<h1 class="t">LIST OF EVENTS</h1>
<% @events.each do |event| %>

  <div class="card-product">
  <%= cl_image_tag event.photo, height: 160, width: 160, crop: :fill %>
  <div class="card-product-infos">
    <h2><%= event.name %></h2>
    <p><%= event[:description] %></p>
    <p><%= link_to 'See more details', event_path(event) %></p>
  </div>
  </div>

<% end %>
