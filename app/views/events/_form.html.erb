<%= simple_form_for [@profile, @event] do |f| %>
  <%= f.error_notification %>

  <%= f.input :name %>
  <%= f.input :start_date %>
  <%= f.input :end_date %>
  <%= f.input :description %>


  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>

  <%= f.button :submit, "Submit" %>
<% end %>
